<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <size
                android:height="20dp"
                android:width="20dp" />
            <solid android:color="@color/warm_grey" />
            <corners android:radius="4dp"/>
        </shape>
    </item>
    <item
        android:bottom="1dp"
        android:left="1dp"
        android:right="1dp"
        android:top="1dp">
        <shape android:shape="rectangle">
            <size
                android:height="19dp"
                android:width="19dp"/>
            <solid android:color="@color/white_three" />
            <corners android:radius="4dp"/>
        </shape>
    </item>
</layer-list>