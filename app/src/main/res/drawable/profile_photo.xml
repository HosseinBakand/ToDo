<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="oval">
            <size
                android:height="22dp"
                android:width="22dp" />
            <solid android:color="@color/warm_grey" />
            <stroke android:color="@color/warm_grey" android:width="1dp"/>
        </shape>
    </item>
    <item
        android:bottom="1dp"
        android:left="1dp"
        android:right="1dp"
        android:top="1dp"
        android:drawable="@drawable/profile_picture"
        android:width="21dp"
        android:height="21dp"/>

</layer-list>